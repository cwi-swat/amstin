schema ParseTreeSchema 
    primitive str 
    primitive int 
    primitive bool 
    class ParseTree path : str top : Tree layout : str end 
    class Tree end 
    class Appl rule : str arg : Tree end 
    class Sequence elements : Tree * end 
    class Create name : str arg : Tree end 
    class Code code : str end 
    class Field name : str arg : Tree end 
    class Value kind : str value : str layout : str end 
    class Lit value : str layout : str end 
    class Ref name : str layout : str end end 
