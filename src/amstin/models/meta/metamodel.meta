metamodel MetaMetaModel

	abstract class Type
	end
	
	class Int < Type
	end
	
	class Str < Type
	end
	
	class Bool < Type
	end
	
	class Id < Type
	end
	
	class Klass < Type
	  klass: Class
	end
	
	class Class < Type
	  name: str
	  isAbstract: bool
	  parent: Parent?
	  fields: Field*
	end
	
	class Parent
	  type: Class
	end
	
	class Field
	  name: str
	  type: Type
	  mult: Mult
	end
	
	abstract class Mult 
	end
	
	class Single < Mult
	end
	
	class Star < Mult
	end
	
	class Opt < Mult
	end
	
	class MetaModel
	  name: str
	  classes: Class*
	end
end


