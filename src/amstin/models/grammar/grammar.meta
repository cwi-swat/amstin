metamodel Format

	class Format
	  name: str
	  start: Rule
	  rules: Rule*
	end
	
	class Rule
	  name: str
	  alts: Alt*
	end
	  
	class Alt
	  type: Class?
	  elements: Element*
	end
	
	class Class
	   name: str
	end
	
	class Label
	  name: str
	end
	
	class Element
	  label: Label?
	  symbol: Symbol
	end
	
	abstract class Symbol
	end
	
	class Sym < Symbol
	   rule: Rule
	end
	
	class Int < Symbol end
	class Str < Symbol end
	class Id < Symbol end
	class Key < Symbol end
	
	class Ref < Symbol
	  name: str
	end
	
	
	class Lit
	  value: str
	end
	
	class IterStar
	  arg: Symbol
	end
	
	class Iter
	  arg: Symbol
	end
	
	class Opt
	  arg: Symbol
	end
	
	class IterStarSep
	  arg: Symbol
	  sep: str
	end
	
	class IterSep
	  arg: Symbol
	  sep: str
	end
end
